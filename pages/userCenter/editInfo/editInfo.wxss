/* pages/userCenter/editInfo/editInfo.wxss */
.edit-info-container {
  width: 100%;
  height: 100vh;
}

.edit-info-container > view {
  box-sizing: border-box;
  border-bottom: #6d1aa7 1rpx solid;
  padding: 50rpx 30rpx;
  height: 127rpx;
}

.edit-info-container > view:last-child {
  border: none;
}

.edit-info-container > view > text {
  font-size: 28rpx;
  color: #333;
}