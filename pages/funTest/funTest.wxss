.fun-test-wrapper{
  background-color: #eeeeee;
  padding: 60rpx 30rpx 30rpx 30rpx;
  min-height: 93.5vh;
}

.test-list-item{
  border-radius: 30rpx;
  margin-bottom: 30rpx;
  padding: 60rpx 40rpx 35rpx 40rpx;
  color: #fff;
  background-size: cover;
  background-position: center center;
}

.test-title{
  font-size: 36rpx;
}

.test-status{
  font-size: 32rpx;
  margin: 20rpx 0 100rpx 0;
}

.test-data-num{
  font-size: 28rpx;
}